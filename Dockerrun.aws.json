{
  "AWSEBDockerrunVersion": "3",
  "volumes": [],
  "containerDefinitions": [
    {
      "name": "web",
      "image": "IMAGE_PLACEHOLDER",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 5000
        }
      ],
      "environment": [
        {
          "name": "FLASK_ENV",
          "value": "production"
        }
      ],
      "links": [
        "redis"
      ]
    },
    {
      "name": "redis",
      "image": "redis:alpine",
      "essential": true,
      "memory": 256
    }
  ]
}

